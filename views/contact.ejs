<!DOCTYPE html>
<html lang="en">
<%- include('partials/header') %>

<body>

<%- include('partials/navbar') %>

<div id="tableContainer">
    <div class="container">
        <form action="https://formspree.io/amkashtechno@gmail.com"
              method="POST">
            <div class="form-group">
                <label for="InputEmail">Email address</label>
                <input type="email" class="form-control" id="InputEmail" aria-describedby="emailHelp" placeholder="Enter email" name="_replyto">
            </div>
            <div class="form-group">
                <label for="InputName">Name</label>
                <input type="text" class="form-control" id="InputName"  placeholder="Enter Name" name="name">
            </div>
            <div class="form-group">
                <label for="InputMessage">Message</label>
                <textarea class="form-control" rows=4 id="InputMessage" placeholder="Enter Message" name="message">
                </textarea>
            </div>
            <button type="submit" class="btn btn-primary btn-orange" value="Send">Send</button>
        </form>
    </div>
</div>

<%- include('partials/footer') %>

<%- include('partials/scripts') %>

</body>
</html>